                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="biobanks" type="xs:IDREFS" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The biobanks that store samples for this collection.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sampleCollections" type="xs:IDREFS" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The subcollections of the sample collection.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="studies" type="xs:IDREFS" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The subcollections of the sample collection.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SampleCollection">
        <xs:annotation>
