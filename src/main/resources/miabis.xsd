            <xs:enumeration value="Familial status">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Sex">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Country of residence">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Ethnic origin">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Population representative sampling">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Lifestyle/Exposure">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

