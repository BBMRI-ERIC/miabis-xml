            <xs:element name="studies" type="xs:IDREFS">
                <xs:annotation>
                    <xs:documentation>
                        The studies that use samples from this collection.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sampleCollections" type="xs:IDREFS">
                <xs:annotation>
                    <xs:documentation>
                        The subcollections of the sample collection.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

