            </xs:enumeration>
            <xs:enumeration value="Gravidity">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Age group">
                <xs:annotation>
                    <xs:documentation>
                        MIABIS-:
                    </xs:documentation>
                </xs:annotation>
